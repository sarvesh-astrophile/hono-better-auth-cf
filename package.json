{
  "name": "hono-better-auth-cf",
  "type": "module",
  "scripts": {
    "start": "wrangler dev",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply hono_better_auth_cf --local",
    "db:migrate:prod": "wrangler d1 migrations apply hono_better_auth_cf --remote",
    "db:check": "drizzle-kit check",
    "db:studio": "drizzle-kit studio",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings"
  },
  "dependencies": {
    "better-auth": "^1.3.7",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.5",
    "hono": "^4.9.5",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250830.0",
    "@types/node": "^24.3.0",
    "drizzle-kit": "^0.31.4",
    "tsx": "^4.20.5",
    "wrangler": "^4.33.1"
  }
}
